pipeline {
    agent any
    tools {
        maven "Maven"
        jdk "Java11"
    }
    stages {
        stage('Initialize'){
            steps{
                echo "PATH = /usr/share/maven/bin:${PATH}"
                echo "M2_HOME = /usr/share/maven"
            }
        }
        stage('Build') {
            steps {
                
                sh 'mvn -B -DskipTests clean package'
                echo "build success and ok"
                }
            }
        }
     }
